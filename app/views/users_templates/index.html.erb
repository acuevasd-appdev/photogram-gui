<h1> List of users </h1>

<table border="1"> 
  <tr>
    <th>ID</th>
    <th>Username</th>
    <th></th>
  </tr>

  <% @list_of_users.each do |this_user|%>
    <tr>
    <td> <%= this_user.id %> </td> 
    <td> <%= this_user.username %> </td>
    <td><a href="/users/<%= this_user.username %>">Show details</a></td>
  </tr>
  <% end %>

</table>
